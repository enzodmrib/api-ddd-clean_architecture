    "@prisma/client": "4.11.0",
    "prisma": "4.11.0",
    "@types/bcryptjs": "2.4.2",
    "bcryptjs": "2.4.3",
    "dayjs": "1.11.7",
    "dotenv": "16.0.3",
    "fastify": "4.17.0",
    "validator": "13.9.0",
    "zod": "3.21.4"
  }
